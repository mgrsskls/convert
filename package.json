{
	"name": "converter",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint:svelte": "svelte-check",
		"lint:prettier": "prettier --check --plugin-search-dir=. .",
		"lint:eslint": "eslint --ignore-path .gitignore .",
		"lint:a11y": "node_modules/.bin/axe http://localhost:3000, http://localhost:3000/colors, http://localhost:3000/currencies, http://localhost:3000/time, http://localhost:3000/units, http://localhost:3000/about-convert --exit --show-errors",
		"lint": "npm run lint:svelte && npm run lint:prettier && npm run lint:eslint && npm run lint:a11y",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"pre-commit": [
		"lint:svelte",
		"lint:prettier",
		"lint:eslint",
		"lint:a11y"
	],
	"devDependencies": {
		"@axe-core/cli": "^4.4.3",
		"@sveltejs/adapter-netlify": "^1.0.0-next.76",
		"@sveltejs/kit": "next",
		"@tsconfig/svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"pre-commit": "^1.2.2",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"selenium-webdriver": "4.3.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.2"
	},
	"type": "module",
	"dependencies": {
		"big.js": "^6.2.1",
		"bignumber.js": "^9.0.2",
		"node-fetch": "^3.2.9"
	}
}
