		"chromedriver": "^124.0.1",
		"eslint": "^9.2.0",
		"install": "^0.13.0",
		"pre-commit": "^1.2.2",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"selenium-webdriver": "4.20.0",
		"svelte": "^4.2.16",
		"svelte-check": "^3.7.1",
