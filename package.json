{
	"name": "converter",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"lint:svelte": "svelte-check",
		"lint:prettier": "prettier --check --plugin-search-dir=. .",
		"lint:eslint": "eslint --ignore-path .gitignore .",
		"lint:a11y": "node_modules/.bin/axe http://localhost:3000, http://localhost:3000/colors, http://localhost:3000/currencies, http://localhost:3000/time, http://localhost:3000/units, http://localhost:3000/about-convert --exit",
		"lint": "npm run lint:svelte && npm run lint:prettier && npm run lint:eslint && npm run lint:a11y",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"pre-commit": [
		"lint:svelte",
		"lint:prettier",
		"lint:eslint",
		"lint:a11y"
	],
	"devDependencies": {
		"@axe-core/cli": "^4.4.1-alpha.247",
		"@sveltejs/adapter-netlify": "^1.0.0-next.45",
		"@sveltejs/kit": "next",
		"@tsconfig/svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"pre-commit": "^1.2.2",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.4.5",
		"svelte-preprocess": "^4.10.3",
		"tslib": "^2.3.1",
		"typescript": "^4.5.5"
	},
	"type": "module",
	"dependencies": {
		"big.js": "^6.1.1",
		"bignumber.js": "^9.0.2",
		"node-fetch": "^3.2.0"
	}
}
