{
	"name": "converter",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint:svelte": "svelte-check",
		"lint:prettier": "prettier --check --plugin-search-dir=. .",
		"lint:eslint": "eslint --ignore-path .gitignore .",
		"lint:a11y": "node_modules/.bin/axe http://localhost:3000, http://localhost:3000/colors, http://localhost:3000/currencies, http://localhost:3000/time, http://localhost:3000/units, http://localhost:3000/about-convert --exit --show-errors",
		"lint": "npm run lint:svelte && npm run lint:prettier && npm run lint:eslint && npm run lint:a11y",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"pre-commit": [
		"lint:svelte",
		"lint:prettier",
		"lint:eslint",
		"lint:a11y"
	],
	"devDependencies": {
		"@axe-core/cli": "^4.7.3",
		"@sveltejs/adapter-netlify": "^2.0.8",
		"@sveltejs/kit": "1.22.4",
		"@tsconfig/svelte": "^5.0.0",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"chromedriver": "^114.0.2",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-svelte": "^2.32.4",
		"install": "^0.13.0",
		"npm": "^9.8.1",
		"pre-commit": "^1.2.2",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^2.10.1",
		"selenium-webdriver": "4.11.1",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.6",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.7"
	},
	"type": "module",
	"dependencies": {
		"big.js": "^6.2.1",
		"bignumber.js": "^9.1.1",
		"node-fetch": "^3.3.2"
	}
}
