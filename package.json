{
	"name": "converter",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint:svelte": "svelte-check",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint .",
