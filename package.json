		"chromedriver": "^142.0.0",
		"eslint": "^9.39.1",
		"install": "^0.13.0",
		"pre-commit": "^1.2.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"selenium-webdriver": "4.38.0",
		"svelte": "^5.43.2",
		"svelte-check": "^4.3.3",
