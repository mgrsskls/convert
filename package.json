{
	"name": "converter",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint:svelte": "svelte-check",
		"lint:prettier": "prettier --check --plugin-search-dir=. .",
		"lint:eslint": "eslint --ignore-path .gitignore .",
		"lint:a11y": "node_modules/.bin/axe http://localhost:3000, http://localhost:3000/colors, http://localhost:3000/currencies, http://localhost:3000/time, http://localhost:3000/units, http://localhost:3000/about-convert --exit --show-errors",
		"lint": "npm run lint:svelte && npm run lint:prettier && npm run lint:eslint && npm run lint:a11y",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"pre-commit": [
		"lint:svelte",
		"lint:prettier",
		"lint:eslint",
		"lint:a11y"
	],
	"devDependencies": {
		"@axe-core/cli": "^4.6.0",
		"@sveltejs/adapter-netlify": "^2.0.4",
		"@sveltejs/kit": "1.15.6",
		"@tsconfig/svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"eslint": "^8.34.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"pre-commit": "^1.2.2",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"selenium-webdriver": "4.8.1",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-preprocess": "^5.0.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4"
	},
	"type": "module",
	"dependencies": {
		"big.js": "^6.2.1",
		"bignumber.js": "^9.1.1",
		"node-fetch": "^3.3.0"
	}
}
